{
  "name": "Weather Forecast Check",
  "nodes": [
    {
      "id": "1",
      "name": "Schedule",
      "type": "n8n-nodes-base.cron",
      "typeVersion": 1,
      "position": [
        250,
        300
      ],
      "parameters": {
        "triggerTimes": {
          "item": [
            {
              "hour": 14,
              "minute": 0
            }
          ]
        }
      }
    },
    {
      "id": "2",
      "name": "Get Forecast",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4,
      "position": [
        450,
        300
      ],
      "parameters": {
        "method": "GET",
        "url": "https://api.weather.com/forecast"
      }
    }
  ],
  "connections": {
    "Schedule": {
      "main": [
        [
          {
            "node": "Get Forecast",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": false
}